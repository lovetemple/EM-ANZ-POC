    stage('check out') {
      steps {
        echo 'checked out'
      }
    }
    stage('compile') {
      steps {
        echo 'compile'
      }
    }
    stage('package') {
      steps {
        echo 'package'
      }
    }
    stage('test') {
      steps {
        echo 'test'
      }
    }
    stage('deploy') {
      steps {
        echo 'deploy'
      }
    }
  }
}